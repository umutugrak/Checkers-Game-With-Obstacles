        <activity
            android:name="com.checkerswithobstacles.About"
            android:label="@string/title_activity_about" >
        </activity>
        <activity
            android:name="com.checkerswithobstacles.BoardSelect"
            android:label="@string/title_activity_board_select" >
        </activity>
        <activity
            android:name="com.checkerswithobstacles.Help"
            android:label="@string/title_activity_help" >
        </activity>
        <activity
            android:name="com.checkerswithobstacles.Board_Activity"
            android:label="@string/title_activity_board_" >
        </activity>
        <activity
            android:name="com.checkerswithobstacles.Generator_Activity"
            android:label="@string/title_activity_generator_" >
        </activity>
    </application>

</manifest>
